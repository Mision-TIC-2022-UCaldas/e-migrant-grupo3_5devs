@page
@using E_Migrant.App.Dominio.Entidades
@model E_Migrant.App.Frontend.Pages.CalificarEntidadModel
@{
}
<h1>@Model.usuario</h1>


<h2>Listado de Entidades</h2>
<table class="table">
    <thead>
        <tr>
            <th>Razón Social</th>
            <th>Nit</th>
            <th>Dirección</th>
            <th>Ciudad</th>
            <th>Telefono</th>
            <th>Correo electronico</th>
            <th>Pagina web</th>
            <th>Sector</th>
            <th>Tipo de servicio</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            
            <tr>
                <td>@Model.entidad.RazonSocial</td>
                <td>@Model.entidad.Nit</td>
                <td>@Model.entidad.Direccion</td>
                <td>@Model.entidad.Ciudad</td>
                <td>@Model.entidad.Telefono</td>
                <td>@Model.entidad.Email</td>
                <td>@Model.entidad.PaginaWeb</td>
                <td>@Model.entidad.Sector</td>
                <td>@Model.entidad.TipoServicio</td>
                <td>
                    <a class="btn btn-primary table-btn" asp-page="./DetallesEntidad" asp-route-idEntidad="@Model.entidad.Id">
                        <i class="bi bi-info-circle" aria-hidden="true"></i>
                        Detalle
                    </a>
                </td>
                <td>
                    <a class="btn btn-secondary table-btn" asp-page="./RegistroEntidad" asp-route-idEntidad="@Model.entidad.Id">
                        <i class="bi bi-pencil-square" aria-hidden="true"></i>
                        Editar
                    </a>
                </td>
                <td>
                    <a class="btn btn-danger table-btn" asp-page="./EliminarEntidad" asp-route-idEntidad="@Model.entidad.Id">
                        <i class="bi bi-trash" aria-hidden="true"></i>
                        Eliminar
                    </a>
                </td>
            </tr>
        
        </tr>
    </tbody>
</table>
<a class="btn btn-primary table-btn" asp-page="./RegistroEntidad">
    <i class="bi bi-file-plus" aria-hidden="true"></i>
    Crear Entidad
</a>
<a class="btn btn-primary table-btn" asp-page="./ListaEntidad">
    <i class="bi bi-file-plus" aria-hidden="true"></i>
    Consultar Entidad
</a>
<a class="btn btn-primary table-btn" asp-page="./RegistrarServicio">
    <i class="bi bi-file-plus" aria-hidden="true"></i>
    Crear servicios
</a>
<a class="btn btn-primary table-btn" asp-page="./Servicio">
    <i class="bi bi-file-plus" aria-hidden="true"></i>
    Consultar servicios
</a>